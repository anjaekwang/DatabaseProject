<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nam_kikim.test">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Some Movie"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ReservationActivity"></activity>
        <activity android:name=".MovieActivity"></activity>
        <activity android:name=".CinemaActivity"></activity>
        <activity android:name=".ActorActivity"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".MatchActivity"></activity>
        <activity android:name=".InformationActivity"></activity>
    </application>

</manifest>